    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "C:/ProgramData/DriverParser/DriverParser-.log",
          "rollingInterval": "Day",
          "buffered": false,
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff} [{Level:u3}] [{ThreadId}] [{SourceContext}] {Message:lj}{NewLine}{Exception}",
          "shared": true
